<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<managed-bean>
		<managed-bean-name>dbAccess</managed-bean-name>
		<managed-bean-class>edu.uic.ids.bean.DbAccess</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>log</managed-bean-name>
		<managed-bean-class>edu.uic.ids.bean.Log</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>edu.uic.ids.bean.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>graphics</managed-bean-name>
		<managed-bean-class>edu.uic.ids.bean.Graphics</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>edu.uic.ids.bean.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>visualAnalysis</property-name>
			<property-class>edu.uic.ids.bean.VisualAnalysis</property-class>
			<value>#{visualAnalysis}</value>
		</managed-property>
		<managed-property>
			<property-name>statisticsBean</property-name>
			<property-class>edu.uic.ids.bean.StatisticsBean</property-class>
			<value>#{statisticsBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dbOperations</managed-bean-name>
		<managed-bean-class>edu.uic.ids.bean.DbOperations</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>edu.uic.ids.bean.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>descStatistics</managed-bean-name>
		<managed-bean-class>edu.uic.ids.bean.DescStatistics</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>regression</managed-bean-name>
		<managed-bean-class>edu.uic.ids.bean.Regression</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>visualAnalysis</managed-bean-name>
		<managed-bean-class>edu.uic.ids.bean.VisualAnalysis</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>edu.uic.ids.bean.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>descStatistics</property-name>
			<property-class>edu.uic.ids.bean.DescStatistics</property-class>
			<value>#{descStatistics}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>statisticsBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.bean.StatisticsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>edu.uic.ids.bean.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>descStatistics</property-name>
			<property-class>edu.uic.ids.bean.DescStatistics</property-class>
			<value>#{descStatistics}</value>
		</managed-property>
		<managed-property>
			<property-name>regression</property-name>
			<property-class>edu.uic.ids.bean.Regression</property-class>
			<value>#{regression}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>export</managed-bean-name>
		<managed-bean-class>edu.uic.ids.bean.Export</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>edu.uic.ids.bean.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mainMenu</managed-bean-name>
		<managed-bean-class>edu.uic.ids.bean.MainMenu</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>edu.uic.ids.bean.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>log</property-name>
			<property-class>edu.uic.ids.bean.Log</property-class>
			<value>#{log}</value>
		</managed-property>
		<managed-property>
			<property-name>statisticsBean</property-name>
			<property-class>edu.uic.ids.bean.StatisticsBean</property-class>
			<value>#{statisticsBean}</value>
		</managed-property>
		<managed-property>
			<property-name>export</property-name>
			<property-class>edu.uic.ids.bean.Export</property-class>
			<value>#{export}</value>
		</managed-property>
		<managed-property>
			<property-name>dbOperations</property-name>
			<property-class>edu.uic.ids.bean.DbOperations</property-class>
			<value>#{dbOperations}</value>
		</managed-property>
		<managed-property>
			<property-name>graphics</property-name>
			<property-class>edu.uic.ids.bean.Graphics</property-class>
			<value>#{graphics}</value>
		</managed-property>
		<managed-property>
			<property-name>dataImport</property-name>
			<property-class>edu.uic.ids.bean.DataImport</property-class>
			<value>#{dataImport}</value>
		</managed-property>
	</managed-bean>

	<navigation-rule>
		<from-view-id>/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/menu.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/menu.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/view.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/statistics.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/import.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/export.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/graphs.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/logs.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
